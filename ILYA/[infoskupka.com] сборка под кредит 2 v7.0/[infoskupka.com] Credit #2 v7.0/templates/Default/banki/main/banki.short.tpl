<div class="banki-short">
	<div class="banki-short-2">
		<div class="mini-bank-img">
			<img src="{image:1->link}" alt="{title}" title="{title}"/>
		</div>
	</div>
	<div class="banki-short-3">
		<div class="mini-bank-title">
			[full-link]{title}[/full-link]
		</div>
		<div class="mini-bank-lic">
			{Bankifield:polnaja-licenzija}
		</div>
	</div>
	<div class="banki-short-4">
		<div class="mini-bank-otziv">
			<i class="icon-phone"></i>{Bankifield:gorjachaja-linija}
		</div>
		<div class="mini-bank-otziv-link">
			[full-link]Оставить отзыв[/full-link]
		</div>
			
	</div>
	<div class="banki-short-5">
		<div class="rang-bank">
			{rate}
		</div>	
	</div>
</div>